{
  "name": "webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn install && ng serve --host 0.0.0.0 --proxy-config proxy.conf.json -o",
    "build": "rm -rf node_modules && yarn install && ng build --prod",
    "lint": "ng lint",
    "lang-extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/hu-HU.json --output ./src/assets/i18n/en-GB.json --output ./src/assets/i18n/ar-AE.json --format namespaced-json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.2",
    "@angular/common": "^6.0.2",
    "@angular/compiler": "^6.0.2",
    "@angular/core": "^6.0.2",
    "@angular/forms": "^6.0.2",
    "@angular/http": "^6.0.2",
    "@angular/platform-browser": "^6.0.2",
    "@angular/platform-browser-dynamic": "^6.0.2",
    "@angular/router": "^6.0.2",
    "chart.js": "^2.7.2",
    "core-js": "^2.5.4",
    "fullcalendar": "3.5.1",
    "intl": "1.2.5",
    "jquery": "3.2.1",
    "lodash": "^4.17.10",
    "moment": "2.18.1",
    "primeng": "^5.2.5",
    "quill": "1.1.8",
    "rxjs": "6",
    "rxjs-compat": "6",
    "web-animations-js": "2.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.3",
    "@angular/cli": "~6.0.3",
    "@angular/compiler-cli": "^6.0.2",
    "@angular/language-service": "^6.0.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.109",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
